// タブストップ機能を使うようなスニペットはCargo Snippetとは別で用意する。
{
  "println": {
    "prefix": "p",
    "body": [
      "println!(\"{}\", $1);"
    ]
  },
  "template": {
    "prefix": "template",
    "body": [
      "use proconio::*;",
      "",
      "fn main() {",
      "  input! {",
      "    n: usize",
      "  }",
      "",
      "  println!(\"{}\", );",
      "}"
    ]
  }
}